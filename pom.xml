	  mvn install:install-file -Dfile=jintellitype-1.3.9.jar -DgroupId=com.melloware.jintellitype -DartifactId=jintellitype -Dversion=1.3.9 -Dpackaging=jar -DgeneratePom=true
	  mvn install:install-file -Dfile=datepicker.jar -DgroupId=com.eltima.components -DartifactId=datepicker -Dversion=1.0 -Dpackaging=jar -DgeneratePom=true
